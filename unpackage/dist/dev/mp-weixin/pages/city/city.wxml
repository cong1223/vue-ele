<view bind:__l="__l"><cmd-nav-bar fixed="{{true}}" background-color="#1296db" left-text="选择城市" back font-color="#fff"></cmd-nav-bar><cmd-nav-bar fixed="{{false}}" background-color="#fff"></cmd-nav-bar><view class="grace-search"><view class="grace-search-in"><view class="grace-search-icon"></view><input type="search" placeholder="城市名称/拼音" data-event-opts="{{[['input',[['searchChange',['$event']]]]]}}" bindinput="__e"/></view></view><block wx:for="{{cityData}}" wx:for-item="cities" wx:for-index="index" wx:key="index"><block><view class="grace-city-AZ" id="{{'grace-city-AZ-'+index}}">{{index}}</view><view class="grace-list"><block wx:for="{{cities}}" wx:for-item="city" wx:for-index="cindex" wx:key="cindex"><view class="items" id="{{'grace-city-list-'+city.code}}"><view class="title" data-cityname="{{city.name}}" data-citycode="{{city.code}}" data-event-opts="{{[['tap',[['chooseCity',['$event']]]]]}}" bindtap="__e">{{city.name}}</view></view></block></view></block></block><view class="grace-city-AZ-right"><block wx:for="{{AZ}}" wx:for-item="char" wx:for-index="index" wx:key="index"><view data-char="{{char}}" data-event-opts="{{[['tap',[['changAZ',['$event']]]]]}}" bindtap="__e">{{char}}</view></block></view></view>